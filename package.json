  "description": "Distance and similarity functions to compare vectors",
  "main": "lib/index.js",
  "module": "./lib-esm/index.js",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "types": "./lib/index.d.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
